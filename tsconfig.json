{
    "compilerOptions": {
        "allowJs": false,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "outDir": "./tmp/dist/",
        "sourceMap": true,
        "module": "commonjs",
        "jsx": "react",
        "target": "es5",
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "lib": ["dom", "es6", "dom.iterable", "scripthost"]
    },
    "exclude": ["node_modules"],
    "awesomeTypescriptLoaderOptions": {
        "useCache": true
    }
}
