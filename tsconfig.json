{
    "compilerOptions": {
        "allowJs": false,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "outDir": "./tmp/dist/",
        "sourceMap": true,
        "module": "commonjs",
        "jsx": "react",
        "target": "es5",
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "lib": ["dom", "es6"]
    },
    "exclude": ["node_modules"]
}
